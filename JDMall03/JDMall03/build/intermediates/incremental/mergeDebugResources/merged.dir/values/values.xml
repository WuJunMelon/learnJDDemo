    <style name="ActivityAnimTheme" parent="android:Theme.Light">
        <item name="android:windowAnimationStyle">@style/FeelyouWindowAnimTheme</item>
    </style>
    <style name="AppBaseTheme" parent="android:Theme.Light"/>
    <style name="AppTheme" parent="AppBaseTheme"/>
    <style name="CustomDialog" parent="android:style/Theme.Dialog">

        <!-- 鑳屾櫙棰滆壊鍙婇�忔槑绋嬪害 -->
        <item name="android:windowBackground">@android:color/transparent</item>
        <!-- 鏄惁鏈夋爣棰� -->
        <item name="android:windowNoTitle">true</item>
        <!-- 鏄惁娴幇鍦╝ctivity涔嬩笂 -->
        <item name="android:windowIsFloating">true</item>
        <!-- 鏄惁妯＄硦 -->
        <item name="android:backgroundDimEnabled">false</item>
        <item name="android:windowFrame">@null</item>
    </style>
    <style name="FeelyouWindowAnimTheme" parent="@android:style/Animation.Activity">
        <item name="android:activityOpenEnterAnimation">@anim/open_enter_anim</item>
        <item name="android:activityOpenExitAnimation">@anim/open_exit_anim</item>
        <item name="android:activityCloseEnterAnimation">@anim/close_exit_anim</item>
        <item name="android:activityCloseExitAnimation">@anim/close_enter_anim</item>
    </style>
    <style name="bottom_ll_style">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_weight">1</item>
        <item name="android:orientation">vertical</item>
        <item name="android:gravity">center</item>
    </style>
    <style name="bottom_text_style">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@drawable/bot_textcolor</item>
        <item name="android:textSize">14sp</item>
        <item name="android:gravity">center</item>
        <item name="android:clickable">true</item>
    </style>
